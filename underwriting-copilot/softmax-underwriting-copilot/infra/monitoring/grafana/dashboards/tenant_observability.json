{
  "id": null,
  "uid": "tenant-observability",
  "title": "Tenant Underwriting Overview",
  "tags": ["tenant", "underwriting"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "templating": {
    "list": [
      {
        "name": "tenant",
        "label": "Tenant",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus_ds"
        },
        "query": "label_values(http_requests_total, tenant_id)",
        "refresh": 1,
        "includeAll": true,
        "allValue": ".*",
        "multi": false,
        "current": {
          "selected": false,
          "text": "All",
          "value": ".*"
        }
      }
    ]
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "p95 HTTP Latency (ms)",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "datasource": {"type": "prometheus", "uid": "prometheus_ds"},
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_ms_bucket{tenant_id=~\"$tenant\"}[5m])) by (le))",
          "legendFormat": "p95"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "min": 0
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Request Throughput (req/s)",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "datasource": {"type": "prometheus", "uid": "prometheus_ds"},
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(http_requests_total{tenant_id=~\"$tenant\"}[5m]))",
          "legendFormat": "Requests"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "min": 0
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "LLM Average Duration (s)",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "datasource": {"type": "prometheus", "uid": "prometheus_ds"},
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(underwriting_llm_duration_seconds_sum{tenant_id=~\"$tenant\"}[5m])) / sum(rate(underwriting_llm_duration_seconds_count{tenant_id=~\"$tenant\"}[5m]))",
          "legendFormat": "LLM"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Job Failure Rate (%)",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "datasource": {"type": "prometheus", "uid": "prometheus_ds"},
      "targets": [
        {
          "refId": "A",
          "expr": "100 * sum(rate(underwriting_jobs_failed_total{tenant_id=~\"$tenant\"}[10m])) / clamp_min(sum(rate(underwriting_jobs_created_total{tenant_id=~\"$tenant\"}[10m])), 1)",
          "legendFormat": "Failure %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0
        },
        "overrides": []
      }
    }
  ]
}
